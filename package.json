{
    "name": "vscode-autohotkey-neko-help-debug",
    "displayName": "AutoHotkeyNekoHelpDebug",
    "description": "AutoHotkey help of VSCode...some code of debug",
    "version": "0.0.1",
    "publisher": "CoffeeChaton",
    "engines": {
        "vscode": "^1.66.0"
    },
    "main": "./dict/extension",
    "activationEvents": [
        "onLanguage:ahk",
        "onDebug"
    ],
    "homepage": "https://github.com/CoffeeChaton/ahkDebug",
    "bugs": {
        "url": "https://github.com/CoffeeChaton/ahkDebug"
    },
    "categories": [
        "Debuggers"
    ],
    "license": "SEE LICENSE IN LICENSE.md",
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#5c2d91",
        "theme": "dark"
    },
    "keywords": [
        "vscode",
        "autohotkey",
        "ahk"
    ],
    "contributes": {
        "breakpoints": [
            {
                "language": "ahk"
            }
        ],
        "debuggers": [
            {
                "type": "ahk",
                "label": "AutoHotkey",
                "program": "./out/debugEntrance.js",
                "runtime": "\"C:\\Program Files\\AutoHotkey\\AutoHotkeyU64.exe\"",
                "configurationAttributes": {
                    "launch": {
                        "required": [
                            "program"
                        ],
                        "properties": {
                            "program": {
                                "type": "string",
                                "description": "Absolute path to a text file.",
                                "default": "${file}"
                            },
                            "runtime": {
                                "type": "string",
                                "description": "Absolute path to a AutoHotkey.exe file.",
                                "default": "C:\\Program Files\\AutoHotkey\\AutoHotkeyU64.exe"
                            },
                            "stopOnEntry": {
                                "type": "boolean",
                                "description": "Automatically stop after launch.",
                                "default": true
                            },
                            "trace": {
                                "type": "boolean",
                                "description": "Enable logging of the Debug Adapter Protocol.",
                                "default": true
                            },
                            "dbgpSettings": {
                                "type": "object",
                                "properties": {
                                    "max_children": {
                                        "type": "integer",
                                        "description": "max number of array or object children to initially retrieve",
                                        "default": 300
                                    },
                                    "max_data": {
                                        "type": "integer",
                                        "description": "max amount of variable data to initially retrieve.",
                                        "default": 131072
                                    }
                                },
                                "description": "Dbgp settings. See https://xdebug.org/docs-dbgp.php#feature-names",
                                "default": {}
                            }
                        }
                    }
                },
                "initialConfigurations": [
                    {
                        "type": "ahk",
                        "request": "launch",
                        "name": "AutoHotkey Debugger",
                        "program": "${file}",
                        "stopOnEntry": true
                    }
                ],
                "configurationSnippets": [
                    {
                        "label": "AutoHotkey Debug: Launch",
                        "body": {
                            "type": "ahk",
                            "request": "launch",
                            "name": "AutoHotkey Debugger",
                            "program": "${file}",
                            "stopOnEntry": true
                        }
                    }
                ]
            }
        ],
        "configuration": {
            "type": "object",
            "title": "AhkNekoHelp",
            "properties": {
                "AhkNekoHelp.Debug.executePath": {
                    "type": "string",
                    "default": "C:/Program Files/AutoHotkey/AutoHotkeyU64.exe",
                    "description": "Execute path of autohotkey."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "node esbuild.config.js",
        "fmt": "dprint fmt",
        "lint": "eslint --fix src",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "test_vsc": "pnpm run compile && node ./node_modules/vscode/bin/test",
        "up": "pnpm up -L"
    },
    "devDependencies": {
        "@types/assert": "^1.5.6",
        "@types/glob": "^7.2.0",
        "@types/micromatch": "^4.0.2",
        "@types/node": "^17.0.29",
        "@types/temp": "^0.9.1",
        "@types/vscode": "^1.66.0",
        "@types/xml2js": "^0.4.11",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.21.0",
        "@typescript/analyze-trace": "^0.9.0",
        "@vscode/test-electron": "^2.1.3",
        "dprint": "^0.27.0",
        "esbuild": "^0.14.38",
        "esbuild-copy-static-files": "^0.1.0",
        "eslint": "^8.14.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-sukka": "^1.3.0",
        "eslint-import-resolver-typescript": "^2.7.1",
        "eslint-plugin-fp": "^2.3.0",
        "eslint-plugin-immutable": "^1.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-no-setter-return": "^0.1.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.29.4",
        "eslint-plugin-react-hooks": "^4.5.0",
        "eslint-plugin-security": "^1.5.0",
        "fs": "0.0.1-security",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "@vscode/debugadapter": "^1.55.1",
        "@vscode/debugprotocol": "^1.55.1",
        "get-port-please": "^2.5.0",
        "xml2js": "^0.4.23"
    }
}
